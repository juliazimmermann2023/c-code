#include <iostream>

using namespace std;

int main()
{
    double num1;
    double num2;
    double multiplicacao;
    cout << "Subtracao de dois numeros \n";
    cout << "Digite o primeiro numero\n";
    cin >> num1;
    cout << "Digite o segundo numero\n";
    cin >> num2;
    multiplicacao = num1 * num2;
    cout << "Resultado:" << multiplicacao;
    return 0;
}
