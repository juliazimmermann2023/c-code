{
   int n;
   cout << " escolha um numero de 1 a 7 \n";
   cin >> n;
   switch (n) {
   case 1 : cout << "Domingo";
   break;
   case 2 : cout << "Segunda";
   break;
   case 3 : cout << "TerÃ§a";
   break;
   case 4 : cout << "Quarta";
   break;
   case 5 : cout << "Quinta";
   break;
   case 6 : cout << "Sexta";
   break;
   case 7 : cout << "Sabado";
   break;

   }
}
